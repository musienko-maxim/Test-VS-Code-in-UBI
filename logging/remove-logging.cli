# Remove the quickstart logger
/subsystem=logging/logger=org.jboss.as.quickstarts.logging:remove

# Remove the file handlers
/subsystem=logging/file-handler=FILE_QS_TRACE:remove
/subsystem=logging/file-handler=FILE_QS_DEBUG:remove
/subsystem=logging/file-handler=FILE_QS_INFO:remove
/subsystem=logging/file-handler=FILE_QS_WARN:remove
/subsystem=logging/file-handler=FILE_QS_ERROR:remove
/subsystem=logging/file-handler=FILE_QS_FATAL:remove

